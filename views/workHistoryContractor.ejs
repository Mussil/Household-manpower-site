<%- include ("partials/navHR.ejs")-%>
<section>

    <link rel = "stylesheet"  type = "text/css"  href = "../css/workingHistoryContractor.css" />
    <!--for demo wrap-->
    <h1>Working history contractor</h1>
    <div class="tbl-header">
        <table cellpadding="0" cellspacing="0" border="0">
            <thead>
            <tr>
                <th>Date</th>
                <th>Shift start time</th>
                <th>Shift end time</th>
                <th>Salary</th>
                <th>Employer</th>
                <th>JobType</th>
                <th>Rating</th>
            </tr>
            </thead>
        </table>
    </div>
    <div class="tbl-content">
        <table cellpadding="0" cellspacing="0" border="0">
            <tbody>
            <% data.forEach(function(data){ %>
                <tr>
                    <%if(String(data.contractor) == String(user.email)) { %>
                        <td><%= data.date.toLocaleDateString() %></td>
                        <td><%= data.startHourShift %></td>
                        <td><%= data.endHourShift %></td>
                        <td><%= data.salary %></td>
                        <td><%= data.employer%></td>
                        <td><%= data.jobType%></td>
                        <td><%= data.rank%></td>
                    <% } %>

                </tr>
            <% } )%>
            </tbody>
        </table>

    </div>
</section>

<%- include ("partials/footer.ejs")-%>